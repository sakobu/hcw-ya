{
  "name": "rpo-suite",
  "version": "0.1.0",
  "description": "Spacecraft relative motion propagation library implementing Yamanaka-Ankersen and Clohessy-Wiltshire algorithms for Rendezvous and Proximity Operations",
  "author": "Sarkis Melkonian",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakobu/hcw-ya.git"
  },
  "homepage": "https://github.com/sakobu/hcw-ya#readme",
  "bugs": {
    "url": "https://github.com/sakobu/hcw-ya/issues"
  },
  "keywords": [
    "orbital-mechanics",
    "spacecraft",
    "rendezvous",
    "proximity-operations",
    "yamanaka-ankersen",
    "clohessy-wiltshire",
    "rpo",
    "astrodynamics",
    "satellite",
    "kepler",
    "relative-motion",
    "state-transition-matrix",
    "ric-frame",
    "lvlh-frame",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "demo": "bun run examples/basic-propagation.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun run test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
